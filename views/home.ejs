<html class="no-js" lang="">
<!--<![endif]-->

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>The Nature Mushroom</title>
    <link rel="icon" href="/images/logo.ico">
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="apple-touch-icon" href="apple-touch-icon.png">

    <link rel="stylesheet" href="assets/css/bootstrap.min.css">
    <link href='https://fonts.googleapis.com/css?family=Pacifico' rel='stylesheet' type='text/css'>

    <!-- Fontawesome 5.14.0 -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.14.0/css/all.min.css" integrity="sha512-1PKOgIY59xJ8Co8+NE6FZ+LOAZKjy+KY8iq0G4B3CyeY6wYHN3yt9PW0XpSriVlkMXe40PTKnXrLnZ9+fkDaog==" crossorigin="anonymous" />

    <!--For Plugins external css-->
    <link rel="stylesheet" href="assets/css/animate/animate.css" />
    <link rel="stylesheet" href="assets/css/plugins.css" />

    <!--Theme custom css -->
    <link rel="stylesheet" href="assets/css/style.css">

    <!--Theme Responsive css-->
    <link rel="stylesheet" href="assets/css/responsive.css" />

    <script src="assets/js/vendor/modernizr-2.8.3-respond-1.4.2.min.js"></script>
    <style>
        #headerName {
            font-size: 4.5rem;
        }
        
        @media screen and (max-width: 600px) {
            #headerName {
                font-size: 3.5rem;
            }
        }
    </style>
</head>

<body>
    <!--[if lt IE 8]>
            <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
        <![endif]-->
    <div class='preloader'>
        <div class='loaded'>&nbsp;</div>
    </div>

    <!-- Start of Header Section -->
    <%- include("partials/header") %>
    <!-- End Header Section -->

    <section id="slider" class="slider">
        <div class="slider_overlay">
            <div class="container">
                <div class="row">
                    <div class="main_slider text-center">
                        <div class="col-md-12">
                            <div class="main_slider_content wow zoomIn" data-wow-duration="1s">
                                <h1 id="headerName">The Nature<br> Mushroom</h1>
                                <p class="wow zoomIn">
                                    <h4>We grow mushrooms by hands so we can say 100% for quality.</h4>
                                    </We>
                                </p>
                                <a href="/shop" class="btn-lg">Shop Now</a>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </section>

    <section id="portfolio" class="portfolio">
        <div class="container">
            <div class="row">
                <div class="portfolio_content text-center  wow fadeIn" data-wow-duration="5s">
                    <div class="col-md-12">
                        <div class="head_title text-center">
                            <h4>Our Range</h4>
                            <h4>Of Mushrooms</h4>
                        </div>
                        <div class="main_portfolio_content">

                            <% products.forEach((product,index) => { %>
                                <div class="col-md-3 col-sm-4 col-xs-10 single_portfolio_text">
                                    <img src="<%= product.image_url %>" alt="" class="thumbnail" style="margin: 2rem; width: 300px;
                                    height: 260px;
                                    object-fit: cover;" />
                                    <div class="portfolio_images_overlay text-center" style="margin-left: 1rem; margin-top:0.3rem; ">
                                        <h6>
                                            <%= product.item_name %>
                                        </h6>
                                        <p class="product_price">&#8377;
                                            <%= product.price%>
                                        </p>
                                        <a href="/shop" class="btn btn-primary">Buy Now</a>
                                    </div>
                                </div>
                                <% })%>



                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <section id="ourPakeg" class="ourPakeg">
        <div class="container">
            <div class="main_pakeg_content">
                <div class="row">
                    <div class="head_title text-center">
                        <h4>Growth</h4>
                        <h4>Procedure</h4>
                    </div>

                    <div class="single_pakeg_one text-right wow rotateInDownRight">
                        <div class="col-md-6 col-md-offset-6 col-sm-8 col-sm-offset-4">
                            <div class="single_pakeg_text">
                                <div class="pakeg_title">
                                    <h4>Initial</h4>
                                </div>
                                <br>
                                <ul>
                                    <li>
                                        <h4>Step 1 - Composting</h4>
                                    </li>
                                    <li>
                                        <h4>Step 2 - Pasturization</h4>
                                    </li>

                                </ul>
                            </div>
                        </div>
                    </div>

                    <div class="single_pakeg_two text-left wow rotateInDownLeft">
                        <div class="col-md-6 col-sm-8">
                            <div class="single_pakeg_text">
                                <div class="pakeg_title">
                                    <h4>Spwan and Growth</h4>
                                </div>
                                <br>

                                <ul>
                                    <li>
                                        <h4>Step 3 - Spawning</h4>
                                    </li>
                                    <li>
                                        <h4>Step 4 - Casing</h4>
                                    </li>

                                </ul>
                            </div>
                        </div>
                    </div>

                    <div class="single_pakeg_three text-left wow rotateInDownRight">
                        <div class="col-md-6 col-md-offset-6 col-sm-8 col-sm-offset-4">
                            <div class="single_pakeg_text">
                                <div class="pakeg_title">
                                    <h4>Snipping and Packing</h4>
                                </div>
                                <br>

                                <ul>
                                    <li>
                                        <h4>Step 5 - Pinning</h4>
                                    </li>
                                    <li>
                                        <h4>Step 6 - Cropping</h4>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <section id="mobaileapps" class="mobailapps">
        <div class="slider_overlay">
            <div class="container">
                <div class="row">
                    <div class="main_mobail_apps_content wow zoomIn">
                        <div class="col-md-5 col-sm-12 text-center">
                            <img src="assets/images/iphone.png" alt="" />
                        </div>
                        <div class="col-md-7 col-sm-12">
                            <div class="single_monail_apps_text">
                                <h4>Happy to Announce </h4>
                                <h1>Mobile App <span>is Available on Google Play Store.</span></h1>

                                <a href=""><img src="assets/images/google.png" alt="" /></a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!--Footer-Widget-->
    <%- include("partials/footer_widget") %> 



    <!--Footer-->
    <%- include('footer'); -%>

    <div class="scrollup">
        <a href="#"><i class="fa fa-chevron-up"></i></a>
    </div>


    <script src="assets/js/vendor/jquery-1.11.2.min.js"></script>
    <script src="assets/js/vendor/bootstrap.min.js"></script>

    <script src="assets/js/jquery-easing/jquery.easing.1.3.js"></script>
    <script src="assets/js/wow/wow.min.js"></script>
    <script src="assets/js/plugins.js"></script>
    <script src="assets/js/main.js"></script>
    <script src="assets/js/fontawesome.min.js"></script>
</body>

</html>